<?xml version="1.0"?>
<package format="2">
  <name>camera</name>
  <version>0.0.0</version>
  <description>The camera package provides nodes to acquire information from a
  Microsoft Kinect device.</description>
  <maintainer email="jane.doe@example.com">Jane Doe</maintainer>
  <license>LGPLv3</license>
  <url>http://ros.org/wiki/camera</url>
  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roscpp</build_depend>
  <exec_depend>roscpp</exec_depend>
  <depend>camera_calibration_parsers</depend>
  <depend>image_view</depend>
  <export>
    <cpp cflags="-I${prefix} -I${prefix}/build"
    lflags="-L${prefix}/lib -Wl,-rpath,${prefix}/lib" />
  </export>
</package>
